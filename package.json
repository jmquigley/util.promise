{
  "name": "util.promise",
  "version": "0.0.2",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Utility functions for handling promises",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.promise"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.promise/issues"
  },
  "engines": {
    "node": ">=7.4"
  },
  "dependencies": {
    "expand-home-dir": "0.0.3",
    "fs-extra": "^2.0.0",
    "lodash": "^4.17.4",
    "object-assign": "^4.1.1",
    "randombytes": "^2.0.3",
    "util.fixture": "0.0.18",
    "util.rstrip": "0.0.7",
    "uuid": "^3.0.1",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "coveralls": "^2.11.15",
    "nyc": "^10.1.2",
    "util.pkgwrap": "0.0.5",
    "xo": "^0.17.1"
  },
  "main": "index.js",
  "scripts": {
    "postinstall": "pkgwrap --postinstall",
    "test": "pkgwrap --testing",
    "report": "pkgwrap --reporting",
    "coverage": "pkgwrap --coverage"
  },
  "xo": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "global": [
      "window",
      "JSON"
    ],
    "rules": {
      "space-before-function-paren": "off",
      "unicorn/filename-case": [
        "error",
        {
          "case": "snakeCase"
        }
      ],
      "arrow-parens": [
        "error",
        "as-needed"
      ]
    }
  }
